<div class="ng-cloak" ng-controller="reports">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Test Report of {{score_res.test[0].test_name}}
        <small></small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content prevent-select">
      <!-- Small boxes (Stat box) -->
      <div class="box">
          <div class="box-body">
      <div class="row">
          <div class="col-xs-12 col-sm-12">
                <ul class="nav nav-tabs">
                    <li ng-class="{ active: isSet(1) }">
                        <a  href="javascript:void(0)" class="notesc" ng-click="setTab(1)" data-toggle="tab">Score Overview</a>
                    </li>
                    <li ng-class="{ active: isSet(2) }">
                        <a  href="javascript:void(0)" class="notesc" ng-click="setTab(2)" data-toggle="tab">Subject Wise</a>
                    </li>
                    <li ng-class="{ active: isSet(3) }">
                        <a  href="javascript:void(0)" class="notesc" ng-click="setTab(3);loadqchart()" data-toggle="tab">Question Wise</a>
                    </li>
                    <li ng-class="{ active: isSet(4) }">
                        <a  href="javascript:void(0)" class="notesc" ng-click="setTab(4)" data-toggle="tab">Time Wise</a>
                    </li>
                    
                    <li ng-if="score_res.test[0].test_type != '0'" ng-class="{ active: isSet(5) }"><a href="javascript:void(0)" class="notesc"
                    ng-click="setTab(5); load_test_topper(0, '69492c549573691a3f079298')" data-toggle="tab">Compare with Topper</a></li>

                    <li ng-class="{ active: isSet(6) }">
                        <a  href="javascript:void(0)" class="notesc" ng-click="setTab(6);changesub()" data-toggle="tab">Solution</a>
                    </li>
                </ul>
                <div class="tab-content ">
                    <div class="tab-pane score_overview" ng-class="{ active: isSet(1) }" id="score_overview">
                        <div class="table-responsive">
                        <table class="table table-striped">
                            <tbody>
                                <tr> 
                                    <th>Max Score of Test</th>
                                    <td>{{score_res.test[0].max_score}}</td>
                                    <th>Total Test Time(Mins)</th>
                                    <td>{{score_res.test[0].time_limit}}</td>
                                    <th>Total Questions</th>
                                    <td>{{score_res.test[0].total_qs}}</td>
                                </tr> 
                                <tr> 
                                    <th>Your Score</th>
                                    <td>{{score_res.total_score}}</td>
                                    <th>You Spend</th>
                                    <td>{{score_res.time_taken|number:2}} Mins.</td>
                                    <th>Correct</th>
                                    <td>{{score_res.correct}}</td>
                                </tr>
                                <tr> 
                                    <th>Percentage</th>
                                    <td>{{(score_res.bonus_marks+score_res.total_score)*100/score_res.test[0].max_score | number:2}}%</td>
                                    <th>Percentile</th>
                                    <td>{{score_res.percentile | number:2}}</td>
                                    <th>Incorrect</th>
                                    <td>{{score_res.incorrect}}</td>
                                </tr>
                                <tr>
                                    <th>Bonus Marks</th>
                                    <td>{{score_res.bonus_marks}}</td>
                                    <th>Rank</th>
                                    <td>
                                        <span ng-if="score_res.test[0].test_type == '1' || score_res.test[0].test_type == '2'">{{score_res.rank}}</span>
                                        <span ng-if="score_res.test[0].test_type =='0'">-NA-</span>
                                   </td>
                                    <th>Unattempted</th>
                                    <td>{{score_res.test[0].total_qs-score_res.correct-score_res.incorrect-score_res.attempted}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                        <hr>
                        <div class="col-xs-12 col-sm-12">
                            <div class="panel panel-flat">
                                <div class="panel-body">
                                    <div class="chart-container text-center">
                                        <div class="display-inline-block" id="testreportchart">
                                            <i class="fa fa-spin fa-spinner"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane reportsubject" ng-class="{ active: isSet(2) }" >
                        <div class="table-responsive">
                         <table class="table  table-striped table-bordered">
                            <thead>
                                <tr> 
                                    <th>Subject</th>
                                    <th>Total Qs</th>
                                    <th>Correct</th>
                                    <th>Incorrect</th>
                                    <th>+Ve Marks</th>
                                    <th>-Ve Marks</th>
                                    <th>Total Marks</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="subjectwise in dd" ng-show="subjectwise._id!='all'">
                                    <td>{{subjectwise.title}}</td>
                                    <td>{{subjectwise.total}}</td>
                                    <td>{{subjectwise.correct}}</td>
                                    <td>{{subjectwise.incorrect}}</td>
                                    <td>{{subjectwise.positive}}</td>
                                    <td>{{subjectwise.negative}}</td>
                                    <td>{{subjectwise.positive+subjectwise.negative}}</td>
                                </tr>
                            </tbody>
                         </table>
                     </div>
                    </div>
                    <div class="tab-pane reportqs" ng-class="{ active: isSet(3) }">
                        <div class=" col-sm-12">
                            <div class="panel panel-flat">
                                <div class="panel-body">
                                    <div class="chart-container">
                                        <div class="chart" id="c3-line-chart">
                                            <i class="fa fa-spin fa-spinner"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <table class="table  table-striped table-bordered">
                            <thead> 
                                <tr>
                                    <th>Q. No.</th>
                                    <th>Correct Ans</th>
                                    <th>Your Ans</th>
                                    <th>Time Takes(Sec.)</th>
                                    <!-- <th>Remarks</th> -->
                                    <th>Status</th>
                                </tr> 
                            </thead>
                            <tbody>
                                <tr ng-repeat="q in questions">
                                    <td>{{$index +1}}</td>
                                    <td ng-if="q.question_type=='SLCT' || q.question_type=='MULTI_SLCT' ||  q.question_type=='MCQ_MULTI_OPTIONS'">
                                        <span ng-repeat="q_opt in q.slct_options" ng-if="q_opt.correct"> {{ q.question_type=='MCQ_MULTI_OPTIONS' ? aplhabet($index) : $index+1 }} </span>
                                    </td>

                                    <td ng-if="q.question_type!='SLCT' && q.question_type!='MULTI_SLCT' && q.question_type!='MCQ_MULTI_OPTIONS' && q.question_type!='FILL_BLANKS'  && q.question_type!='SENTENCE_ARRANGMENT'">
                                        {{q.ans}}
                                    </td>
                                    <td ng-if="q.question_type=='FILL_BLANKS'">
                                            <div ng-show="q.question_type=='FILL_BLANKS'">
        <b>ANS: </b>
        <div ng-repeat="ans in q.ans">
            <div>Blank {{ans.id}} : {{ ans.ans }}</div>
        </div>
    </div>
                                        
                                    </td>
                                    <td ng-if="q.question_type=='SENTENCE_ARRANGMENT'">
                                            <div ng-show="q.question_type=='SENTENCE_ARRANGMENT'">
        <b>ANS: </b>
        <div ng-repeat="ans in q.ans">
            <div>{{ans.id+1}} - {{ ans.value }}</div>
        </div>
    </div>
                                    </td>


                                    <td>
                                        <span ng-if="q.question_type!='AUDIO_TYPE' && q.question_type!='FILL_BLANKS'  && q.question_type!='SENTENCE_ARRANGMENT'">{{ q.question_type=='MULTI_SLCT' ? q.std_ans.join(", ") : q.question_type=='SLCT' ? num(q.std_ans)+1 : q.question_type=='MCQ_MULTI_OPTIONS' ? aplhabet(q.std_ans)  : q.question_type=='TXT_INPUT' ? q.std_ans.join(",") : q.std_ans }}</span>
                                        <audio controls controlsList="nodownload" src="https://test-audio-answers1.s3.ap-south-1.amazonaws.com/{{q.std_ans}}" ng-if="q.question_type=='AUDIO_TYPE'"></audio>
                                        
                                            <div ng-show="q.question_type=='FILL_BLANKS'">
        <b>ANS: </b>
        <div ng-repeat="(index,ans) in q.std_ans">
            <div ng-if="ans">Blank {{index}}: {{ ans }}</div>
        </div>
    </div>
                                            <div ng-show="q.question_type=='SENTENCE_ARRANGMENT'">
        <b>ANS: </b>
        <div ng-repeat="ans in q.std_ans">
            <div>{{ans.id+1}} - {{ ans.value }}</div>
        </div>
    </div>
                                    </td>
                                    <td>{{q.time_taken}}</td>
                                    <!-- <td>{{q.remarks}}</td> -->
                                    <td>{{q.ans_status}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="tab-pane reporttime" ng-class="{ active: isSet(4) }">
                        <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th style="width: 40%">Subject</th>
                                    <th ng-repeat="sub in dd">{{sub.title}}</th>
                                </tr>
                            </thead>
                            <tbody>
                                <th>Time Taken (Mins.)</th>
                                <th ng-repeat="sub in dd">{{sub.time_taken/60|number:2}} Mins.</th>
                            </tbody>
                        </table>
                    </div>
                    </div>
                    <div class="tab-pane reportcompare" ng-class="{ active: isSet(5) }"  id="compare">
                        <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Student Name</th>
                                    <th>Correct</th>
                                    <th>InCorrect</th>
                                    <th>Score</th>
                                    <th>Per%</th>
                                    <th>Rank</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="topper in toppers">
                                    <td>{{topper.student[0].firstname}} {{topper.student[0].lastname}}</td>
                                    <td>{{topper.correct}}</td>
                                    <td>{{topper.incorrect}}</td>
                                    <td>{{topper.total_score}}</td>
                                    <td>{{topper.total_score*100/topper.test[0].max_score | number:2}}%</td>
                                    <td>{{j + $index}}</td>
                                </tr>
                              <!-- <tr ng-show="toppers.length"><td  colspan="6"> <ul id="paginate" class="pagination"></ul></td></tr> -->
                                <tr ng-show="!toppers.length"><td  colspan="6"> No Record Found</td></tr>
                            </tbody>
                        </table>
                    </div>
                    </div>
                    <div class="tab-pane reportsolution" ng-class="{ active: isSet(6) }"  id="solution">
                        <div class="col-xs-12 col-sm-12">
                            <div class="row m-b-20 m-t-20">
                            <div class="col-xs-6 col-md-6 ">
