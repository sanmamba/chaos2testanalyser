  }
});
        
        
        $('body').on('click','#paginate a',function(e){
           
            if(!$(this).parent().hasClass('active'))
            {
                e.preventDefault();
                var page = ($(this).attr("href")).split('/');
                var page_number = page[page.length-1];
                if(page_number)
                $scope.load_test_topper(page_number,'69492c549573691a3f079298');
                else{
                    $scope.load_test_topper(0,'69492c549573691a3f079298');
                }
            }
            else{
                $scope.load_test_topper(0,'69492c549573691a3f079298');
            }
                return false;
       });
        
        $scope.loadchart = function (data)
        {
            var arr =[];
            arr[0] = ['Correct',data.correct];
            arr[1] = ['Incorrect',data.incorrect];
            arr[2] = ['Unattempted',parseInt(data.test[0].total_qs-data.correct-data.incorrect)];
            load_chart(arr,'testreportchart');
        }
        
        $scope.loadqchart = function()
        {
            if($scope.chartclk)
            {
                var data = $scope.questions;
                var arr2 =[];
                var xarr = [];
                arr2[0]='Time Taken: ';
                var len = data.length;
                for(var j=0;j<len;j++)
                {
                    xarr[j] = j+1;
                    arr2[j+1] = parseInt(data[j].time_taken);
                }
                load_linechart(arr2,xarr);
                $scope.chartclk = false;
            }
        }
      });
    function load_chart(data,id)
    {
        c3.generate({
            bindto: '#'+id,
            size: { width: 450 },
            color: {
                pattern: ['#00a65a', '#dd4b39','#D3D3D3']
            },
            data: {
                columns: data,
                type : 'pie'
            },legend: {
        position: 'right'
    }
        });
    }
    
    function load_linechart(data,xarr)
    {
        setTimeout(function(){
        c3.generate({
            bindto: '#c3-line-chart',
            data: {
                columns: [
                    data
                ],
                type: 'bar'
            },axis: {
        x: {
            type: 'category',
            categories: xarr
        }},
            bar: {
                width: {
                    ratio: 0.5 // this makes bar width 50% of length between ticks
                }
                // or
                //width: 100 // this makes bar width 100px
            }
        });
        },500)
    }

